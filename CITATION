CITATION.cff

citHeader("To cite tropTongue in publications use:")

citEntry(entry = "Article",
  title        = "Improving allometric models to estimate the proboscis length of tropical bees",
  author       = personList(as.person("Kilian Frühholz"),as.person("Kenneth Kuba"), as.person("Maximilian Pitz"), as.person("Julia Windl"), as.person("Alexander Keller"), as.person("Gunnar Brehm"), as.person("Claus Rasmussen"), as.person("Sara Diana Leonhardt"), as.person("Ugo Mendes Diniz")),
  journal      = "",
  year         = "2025",
  volume       = "",
  number       = "",
  pages        = "",
  url          = "",

  textVersion  =
  paste("Improving allometric models to estimate the proboscis length of tropical bees",
        "2025",
        "Frühholz, Kuba, Pitz, Windl, Keller, Brehm, Rasmussen, Leonhardt, Diniz")
)
